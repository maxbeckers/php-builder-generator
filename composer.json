{
  "name": "maxbeckers/php-builder-generator",
  "description": "Generate builder patterns for PHP classes using attributes",
  "type": "composer-plugin",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Max Beckers",
      "email": "beckers.maximilian@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "composer-plugin-api": "^2.0",
    "symfony/console": "^6.0|^7.0",
    "symfony/finder": "^6.0|^7.0",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "composer/composer": "^2.0",
    "symfony/filesystem": "^6.0|^7.0"
  },
  "autoload": {
    "psr-4": {
      "MaxBeckers\\PhpBuilderGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MaxBeckers\\PhpBuilderGenerator\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  },
  "extra": {
    "class": "MaxBeckers\\PhpBuilderGenerator\\Plugin\\ComposerPlugin"
  },
  "bin": [
    "bin/php-builder-generator"
  ]
}
